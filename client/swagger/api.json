{
  "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
  "swagger": "./swagger/api/swagger.json",
  "output": "./src/app/sdk/microservices/api",
  "prefix": "Api",
  "enumModule": true,
  "apiModule": true,
  "camelCase": true,
  "minParamsForContainer": 2,
  "defaultTag": "Api",
  "removeStaleFiles": true,
  "modelIndex": true,
  "serviceIndex": true
}
